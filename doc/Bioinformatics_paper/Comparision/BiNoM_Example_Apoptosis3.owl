    <organism rdf:resource="http://www.reactome.org/biopax#Homo_sapiens"/>
    <xref>
      <UnificationXref rdf:about="http://www.reactome.org/biopax#UniProt_P10636">
        <db rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >UniProt</db>
        <id rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
